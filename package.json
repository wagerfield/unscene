{
  "name": "unscene",
  "license": "MIT",
  "version": "0.0.5",
  "homepage": "https://unscene.vercel.app",
  "description": "2D graphics library for the web",
  "repository": "https://github.com/wagerfield/unscene.git",
  "bugs": "https://github.com/wagerfield/unscene/issues",
  "author": {
    "name": "Matthew Wagerfield",
    "email": "matthew@wagerfield.com",
    "url": "https://matthew.wagerfield.com"
  },
  "keywords": [
    "2D",
    "graphics",
    "drawing",
    "canvas",
    "webgpu",
    "webgl",
    "svg"
  ],
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": false,
  "main": "./dist/unscene.cjs",
  "module": "./dist/unscene.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/unscene.js",
      "require": "./dist/unscene.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "docs": "astro dev",
    "check": "astro check",
    "build": "astro check && astro build",
    "clean": "rm -rf .astro .vercel dist",
    "format": "prettier --write .",
    "preview": "astro preview",
    "package": "vite build",
    "release": "release-it",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --write",
      "vitest related --run"
    ],
    "**/*.{json,md,mdx}": [
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "prettier": {
    "semi": false
  },
  "devDependencies": {
    "@astrojs/check": "0.4.1",
    "@astrojs/starlight": "0.17.2",
    "@commitlint/cli": "18.6.0",
    "@commitlint/config-conventional": "18.6.0",
    "@webgpu/types": "0.1.40",
    "astro": "4.3.1",
    "husky": "9.0.10",
    "lint-staged": "15.2.1",
    "prettier": "3.2.4",
    "release-it": "17.0.3",
    "sharp": "0.33.2",
    "typescript": "5.3.3",
    "vite": "5.0.12",
    "vite-plugin-dts": "3.7.2",
    "vitest": "1.2.2"
  }
}
